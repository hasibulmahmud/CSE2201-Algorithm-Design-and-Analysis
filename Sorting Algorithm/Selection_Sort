#include<bits/stdc++.h>
using namespace std;

int main(){
    int arr[]={6,5,4,3,2,1};

    for(int i=0;i<5;i++){
        int min_val=i;

        for(int j=i+1;j<6;j++){
            if(arr[j]<arr[min_val]){
                min_val = j;
            }
        }
        swap(arr[i],arr[min_val]);
    }
     for(int i=0;i<6;i++){
        cout << arr[i] <<" ";
    }
    cout << endl;
}
